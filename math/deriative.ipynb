   "source": [
    "# 1. x^2 + y^2 를 일반 미분하기\n",
    "\n",
    "def f(x):\n",
    "    return x**2\n",
    "\n",
    "def deriative(f, x, h=1e-5):\n",
    "    return (f(x+h) - f(x))/h\n"
   ]
  },
